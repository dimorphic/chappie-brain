{
  "name": "Chappie-Brain",
  "version": "0.0.1",
  "description": "Chappie Brain / Consciousness remake in React / Angular / Canvas",
  "homepage": "https://github.com/dimorphic/chappie-brain",
  "author": "Sabin Tudor <contact@sabin-tudor.ro>",
  "license": "GNU GPL V3",
  "main": "npm start",
  "scripts": {
    "start": "npm run start:dev",
    "postinstall": "npm run typings-install",
    "build": "rimraf build && npm run build:prod",
    "clean": "npm cache clean && rimraf node_modules typings build",
    "start:dev": "NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.js --progress --profile --colors --watch --hot --display-error-details --display-cached",
    "build:prod": "NODE_ENV=production webpack --config webpack/webpack.prod.js --progress --profile --colors --display-error-details --display-cached",
    "icons": "svg-sprite --symbol --symbol-sprite='spritesheet.html' --symbol-dest='svg' --svg-xmldecl=false --svg-doctype=false  --dest='./src/assets/' ./src/assets/svg/*.svg",
    "typings-install": "typings install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "katalyst",
    "boilerplate",
    "html",
    "css",
    "javascript"
  ],
  "devDependencies": {
    "angular2-hmr": "^0.7.0",
    "awesome-typescript-loader": "^1.0.0",
    "codelyzer": "0.0.23",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.1.2",
    "es6-promise-loader": "^1.0.1",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.14.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "normalize.css": "^4.0.0",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "svg-sprite": "^1.2.19",
    "ts-helper": "0.0.1",
    "ts-node": "^0.9.0",
    "tslint": "^3.6.0",
    "tslint-loader": "^2.1.3",
    "typescript": "^1.8.9",
    "typings": "^1.0.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "3.0.0-alpha.3",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "core-js": "^2.4.0",
    "noisejs": "^2.1.0",
    "rxjs": "5.0.0-beta.8",
    "stats.js": "^0.16.0",
    "zone.js": "^0.6.12"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
